{
  "name": "ts-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for a TypeScript project",
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "prepack": "pnpm run build"
  },
  "keywords": [],
  "author": "Tyrunas Jokubauskas",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {},
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,css,scss}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
